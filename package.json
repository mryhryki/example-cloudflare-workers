{
  "name": "example-cloudflare-workers",
  "author": "mryhryki",
  "license": "MIT",
  "description": "Example for CloudFlare Workers",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "login": "wrangler login",
    "dev": "wrangler dev",
    "publish": "run-s build wrangler:publish",
    "wrangler:publish": "wrangler publish",
    "test": "curl https://example.mryhryki.workers.dev/api/health",
    "build": "esbuild --bundle --minify --platform=node --target=node14 --outfile=dist/index.js src/index.ts",
    "check": "run-s lint type",
    "lint": "mryhryki-lint",
    "lint:fix": "mryhryki-lint-fix",
    "type": "tsc",
    "type:watch": "tsc --watch"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^3.7.1",
    "@mryhryki/datetime": "^0.0.4",
    "@mryhryki/lint": "^0.0.6",
    "@types/jest": "^27.4.1",
    "@types/service-worker-mock": "^2.0.1",
    "esbuild": "^0.14.38",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@cloudflare/wrangler": "^1.19.12"
  }
}
